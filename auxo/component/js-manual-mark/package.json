{
  "name": "react-boilerplate-ie8",
  "version": "1.0.0",
  "description": "支持IE8的react脚手架",
  "scripts": {
    "copy": "copyfiles -f ./index.html ./favicon.ico ./dist",
    "copy:polyfill": "copyfiles -f ./polyfill/** ./dist/polyfill",
    "start": "cross-env env=dev node server.js",
    "start:mock": "cross-env env=dev node server.js --mock",
    "dev": "cross-env env=dev node server.js",
    "dev:mock": "cross-env env=dev node server.js --mock",
    "ie8": "cross-env env=dev-ie8 node server.js",
    "ie8:mock": "cross-env env=dev-ie8 node server.js --mock",
    "dist": "cross-env env=dist npm run copy & npm run copy:polyfill & webpack",
    "mock": "json-server --watch ./mock/db.json --routes ./mock/routes.json --port 8080",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false ",
    "compiler": "webpack --progress -p",
    "build": "npm run compiler && gulp copy:polyfill",
    "build:development": "cross-env env=dist CDN=development npm run build",
    "build:test": "cross-env env=dist CDN=test npm run build",
    "build:preproduction": "cross-env env=dist CDN=preproduction npm run build",
    "build:product": "cross-env env=dist CDN=product npm run build",
    "build:aws": "cross-env env=dist CDN=aws npm run build",
    "build:aws-california": "cross-env env=dist CDN=aws-california npm run build",
    "build:all": "npm run build:development && npm run build:test && npm run build:preproduction && npm run build:product && npm run build:aws && npm run build:aws-california"
  },
  "author": "xudaosong <xudaosong@163.com>",
  "license": "MIT",
